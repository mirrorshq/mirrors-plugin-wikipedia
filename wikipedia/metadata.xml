<?xml version="1.0" encoding="utf-8"?>
<mirror-site id="wikipedia">
  <name>Wikipedia</name>
  <storage type="file"/>
  <storage type="mariadb"/>
  <advertiser type="mediawiki"/>
  <initializer>
    <executable>updater.py</executable>
  </initializer>
  <updater>
    <executable>updater.py</executable>
    <schedule type="interval">1w</schedule>
  </updater>
</mirror-site>
